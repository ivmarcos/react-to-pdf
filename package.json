{
  "name": "react-to-pdf",
  "version": "0.0.14",
  "description": "Create pdf documents from React Components",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "keywords": [
    "pdf",
    "react",
    "convert"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivmarcos/react-to-pdf.git"
  },
  "author": "Marcos Andrei Ivanechtchuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivmarcos/react-to-pdf/issues"
  },
  "homepage": "https://github.com/ivmarcos/react-to-pdf#readme",
  "scripts": {
    "start": "parcel example/index.html",
    "build": "parcel build",
    "clean": "rimraf dist",
    "pre:build": "npm run test && npm run clean",
    "docs": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts src/types.ts src/constants.ts",
    "test": "jest",
    "format": "prettier --write src example README.md CHANGELOG.md"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "canvas": "^2.11.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "parcel": "latest",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "react": "^16.8",
    "react-dom": "^16.8",
    "release-it": "^15.10.1",
    "rimraf": "^5.0.0",
    "typedoc": "^0.24.4",
    "typedoc-plugin-markdown": "^3.15.2",
    "typescript": ">=3.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1"
  }
}
